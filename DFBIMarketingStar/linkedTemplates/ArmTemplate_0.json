{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nome do Data Factory",
			"defaultValue": "DFBIMarketingStar"
		},
		"ServidorBIMarketingStar_password": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'password' de 'ServidorBIMarketingStar'"
		},
		"ServidorBIMarketingstar__password": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'password' de 'ServidorBIMarketingstar_'"
		},
		"ServidorBIMarketingStar_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "servidorbimarketingstar.database.windows.net"
		},
		"ServidorBIMarketingStar_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbbimarketingstars"
		},
		"ServidorBIMarketingStar_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsql"
		},
		"ServidorBIMarketingstar__properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "servidorbimarketingstar.database.windows.net"
		},
		"ServidorBIMarketingstar__properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbbimarketingstars"
		},
		"ServidorBIMarketingstar__properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsql"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/ServidorBIMarketingStar')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"server": "[parameters('ServidorBIMarketingStar_properties_typeProperties_server')]",
					"database": "[parameters('ServidorBIMarketingStar_properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('ServidorBIMarketingStar_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('ServidorBIMarketingStar_password')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ServidorBIMarketingstar_')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"server": "[parameters('ServidorBIMarketingstar__properties_typeProperties_server')]",
					"database": "[parameters('ServidorBIMarketingstar__properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('ServidorBIMarketingstar__properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('ServidorBIMarketingstar__password')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/dsTBBackup')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ServidorBIMarketingStar",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "id",
						"type": "int",
						"precision": 10
					},
					{
						"name": "nome",
						"type": "varchar"
					},
					{
						"name": "descricao",
						"type": "varchar"
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "backup"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ServidorBIMarketingStar')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/dsTBOriginal')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ServidorBIMarketingStar",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [
					{
						"name": "id",
						"type": "int",
						"precision": 10
					},
					{
						"name": "nome",
						"type": "varchar"
					},
					{
						"name": "descricao",
						"type": "varchar"
					}
				],
				"typeProperties": {
					"schema": "dbo",
					"table": "original"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ServidorBIMarketingStar')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/PipelineBackup_Original')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Original Backup",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "AzureSqlSource",
								"queryTimeout": "02:00:00",
								"partitionOption": "None"
							},
							"sink": {
								"type": "AzureSqlSink",
								"writeBehavior": "insert",
								"sqlWriterUseTableLock": false
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "id",
											"type": "Int32",
											"physicalType": "int"
										},
										"sink": {
											"name": "id",
											"type": "Int32",
											"physicalType": "int"
										}
									},
									{
										"source": {
											"name": "nome",
											"type": "String",
											"physicalType": "varchar"
										},
										"sink": {
											"name": "nome",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "descricao",
											"type": "String",
											"physicalType": "varchar"
										},
										"sink": {
											"name": "descricao",
											"type": "String",
											"physicalType": "varchar"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "dsTBOriginal",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "dsTBBackup",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"folder": {
					"name": "Backup"
				},
				"annotations": [],
				"lastPublishTime": "2024-07-16T21:11:33Z"
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/dsTBOriginal')]",
				"[concat(variables('factoryId'), '/datasets/dsTBBackup')]"
			]
		}
	]
}